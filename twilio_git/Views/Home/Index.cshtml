<div class="container">
  <div class="row">
    <div class="col-sm-10 col-md-10">
      <div class="col-sm-6 col-md-6 row-md-6">
        <div class="panel panel-default">
          <div class="panel-body" style="min-width=225px;">
			<h3>Made possible by Twilio</h3>
            <form id="contactform" role="form" action="/Home/Call/" method="post">
              <label class="control-label col-sm-4">From</label>
              <div class="form-group">
                <input class="form-control"
                  type="text"
                  name="userNumber"
                  id="userNumber"
                  min-lenght="12"
                  max-lenght="12"
                  placeholder="+371XXXXXXXX">
              </div>
              <label class="control-label col-sm-4">To</label>
              <div class="form-group">
                <input class="form-control"
                  type="text"
                  name="endNumber"
                  id="endNumber"
                  min-lenght="12"
                  max-lenght="12"
                  placeholder="+371YYYYYfgdhYYY">
              </div>
              <button type="submit" class="btn btn-success">Call</button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>